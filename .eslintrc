{
    "plugins": [
        "standard",
        "promise",
        "react",
        "jsx-a11y",
        "import"
    ],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "root": true,
    "env": {
        "es6": true,
        "amd": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "ecmaFeatures": { "modules": true },
    "rules": {
        "arrow-spacing": 1,
        "block-scoped-var": 1,
        "brace-style": [1, "1tbs", {"allowSingleLine": true}],
        "camelcase": 1,
        "comma-dangle": 1,
        "comma-style": [1, "last"],
        "complexity": [1, 20],
        "curly": [1, "all"],
        "dot-notation": [1, { "allowKeywords": true }],
        "eqeqeq": [1, "allow-null"],
        "guard-for-in": 1,
        "indent": [2, 2, {"SwitchCase": 1}],
        "linebreak-style": [1, "unix"],
        "max-depth": [1, 4],
        "max-len": [1, 150, 4],
        "max-params": [1, 3],
        "max-statements": [1, 10],
        "new-cap": 1,
        "no-bitwise": 1,
        "no-caller": 1,
        "no-cond-assign": [1, "except-parens"],
        "no-console": 1,
        "no-debugger": 1,
        "no-empty": 1,
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-parens": 1,
        "no-extra-semi": 1,
        "no-irregular-whitespace": 1,
        "no-iterator": 1,
        "no-loop-func": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-multi-str": 1,
        "no-new": 1,
        "no-plusplus": [1, { "allowForLoopAfterthoughts": true }],
        "no-proto": 1,
        "no-script-url": 1,
        "no-sequences": 1,
        "no-shadow": 1,
        "no-undef": 1,
        "no-unused-vars": 1,
        "no-with": 1,
        "quotes": [1, "single"],
        "semi": [2, "always"],
        "strict": [1],
        "valid-typeof": 1,
        "wrap-iife": [1, "inside"]
    },
    "output-file": "static_analysis.txt"
}
